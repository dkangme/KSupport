<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuario" table="Usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreCompleto" type="string" column="nombre_completo" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correo" type="string" column="correo" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="unidad" type="string" column="unidad" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="anexo" type="string" column="anexo" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
